<p>
  <%="#{address.firstname} #{address.lastname} (#{address.number}) #{address.phone}" %><%= address.alternative_phone unless address.alternative_phone.blank? %><br />
  <%= formated_cpf @order.user.cpf unless @order.user.cpf.blank? %><br />
  <%= address.address1 %> <%= address.number %> <%= ", #{address.address2}" unless address.address2.blank? %>
  <%= "- #{address.neighborhood}" unless address.neighborhood.blank? %>
  <%= ", #{address.city}, #{address.state_text}, #{address.zipcode}, #{address.country.name}" %>
</p>
